{
    "version": 2,
    "name": "camr-rank",
    "functions": {
        "app/**/route.{ts,js}": {
            "regions": [
                "gru1"
            ],
            "maxDuration": 10
        },
        "api/**/*.js": {
            "regions": [
                "gru1"
            ],
            "maxDuration": 10
        }
    },
    "regions": [
        "gru1"
    ],
    "headers": [
        {
            "source": "/images/(.*)",
            "headers": [
                {
                    "key": "Cache-Control",
                    "value": "public, max-age=31536000, immutable"
                }
            ]
        },
        {
            "source": "/api/(.*)",
            "headers": [
                {
                    "key": "Cache-Control",
                    "value": "no-cache, no-store, must-revalidate"
                }
            ]
        }
    ]
}